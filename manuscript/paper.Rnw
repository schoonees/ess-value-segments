xlims2 <- 1.05 * range(Items[Items$time < 7, "x"], Ratings[, c("x", "y")])
xlims1 <- 1.05 * range(Items[, "x"], Ratings[, c("x", "y")])
ylims2 <- 1.05 * range(Items[Items$time < 7, "y"], Ratings[, c("x", "y")])
ylims1 <- 1.05 * range(Items[, "y"], Ratings[, c("x", "y")])
xlim <- c(replicate(6, xlims2, simplify = FALSE), list(xlims1))
ylim <- c(replicate(6, ylims2, simplify = FALSE), list(ylims1))
nr.lab <- c(11, 12, 11, 10, 6, 9, 21)
biplnms <- paste("Segment", segnms)
# c("Segment I", "SII", "3: Traditionalists", "4: Universalists", 
                  # "5: Indifferent", "6: Entrepreneurs", "7: Uninterested")
for (i in 1:7) {
  ## Label only the largest effects
  tmp <- data.frame(e=apply(tcrossprod(as.matrix(Ratings[,-1]), 
                                           as.matrix(subset(Items[, c("time", "x", "y")], subset = time == i)[,-1])), 2, 
                                function(x) max(abs(x))), 
                        n=factor(Segments$Value, order = TRUE))
      ord <- order(tmp[,1], decreasing = TRUE)
      df <- data.frame(tmp[ord, ], o = ord)
      keep <- rep(FALSE, nrow(Segments))
      keep[df$o[1:nr.lab[i]]] <- TRUE
      
  ## Make plots
  bipl[[i]] <- ggplot(data = subset(Items, time == i), mapping = aes(x = x, y = y)) + 
    geom_segment(data = Ratings, aes(xend = x, yend = y), x = 0, y = 0, arrow = arrow(length = unit(0.15,"cm"))) +
    geom_point(mapping = aes(fill = Domain, shape = ItemNr), size = 3.5) +
    scale_shape_manual(values = 21:23) +
    guides(fill = guide_legend(override.aes = list(shape = 21)), shape = guide_legend(title = "Item Nr")) + 
    xlim(xlim[[i]]) + ylim(ylim[[i]]) + coord_equal() +
    theme_bw() + theme(panel.grid = element_blank()) + xlab("Dim. 1") + ylab("Dim. 2") +
    geom_text(data = subset(Items, subset = time == i & keep), 
                  aes(label = Value), hjust = Settings$hjust[keep,i], 
                  vjust = Settings$vjust[keep,i], size = 4) + 
    geom_text(data = Ratings, aes(x = x, y = y, label = Label, 
                                      vjust = c(-1, 1.5, 1.5, 1.5, 1.5, -0.5, -0.5), 
                                      hjust = c(0.6, 0.2, 0.25, 0.9, 0.9, 0.5, 0.5)), size = 3) +
    ggtitle(paste0(biplnms[i], " (",formatC(100 * segprop[i], format = "f", digits = 1), "%)"))
  if (i < 7) bipl[[i]] <- bipl[[i]] + theme(legend.position = "none")
}
