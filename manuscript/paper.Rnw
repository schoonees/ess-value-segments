<<mlogit-ess, message = FALSE, include=FALSE>>=
## Use NLParty data with weights

## Create new columns for NLParty to recode the demographics
## Age
NLParty$Age <- NLParty$age_cat3
levels(NLParty$Age) <- c("15--34", "35--59", "60--96")
  # c("Young", "Mature", "Older")
NLParty$Age <- relevel(NLParty$Age, ref = "60--96")

## Education
NLParty$Education <- NLParty$eduyrs_cat3
levels(NLParty$Education) <- c("1--10", "11--15", "16--30")
  # c("Low", "Medium", "High")
NLParty$Education <- relevel(NLParty$Education, ref = "16--30")

## Gender
NLParty$Gender <- NLParty$gndr
NLParty$Gender <- relevel(NLParty$Gender, ref = "Female")

## Create response variable Orientation using the lookup table from chunk 25
## Note: There are missings for people for whom vote is missing
NLParty$Orientation <- lookup_orientation[as.character(NLParty$party.nv)]
NLParty$Orientation <- factor(NLParty$Orientation)
NLParty$Orientation <- relevel(NLParty$Orientation, ref = "None")

## Create categorical version of Wave
NLParty$Wave <- factor(NLParty$ESSRound)

## Do the multinomial logit models to model Orientation
require(nnet)

## Intercept only
mlog_int <- multinom(Orientation ~ 1, weights = reweight.nv, data = NLParty)
summary(mlog_int) 

## Demographics only
mlog_dem <- multinom(Orientation ~ Gender + Education + Age, weights = reweight.nv, data = NLParty)
summary(mlog_dem)

## Demographics and wave
mlog_dem_wave <- multinom(Orientation ~ Gender + Education + Age + Wave, weights = reweight.nv, 
                          data = NLParty)
summary(mlog_dem_wave)

## Demographics, wave and segments
mlog_dem_wave_segments <- multinom(Orientation ~ Gender + Education + Age + Wave + 
                                    Seg1 + Seg2 + Seg3 + Seg4 + Seg5 + Seg6, 
                                  weights = reweight.nv, data = NLParty)
summary(mlog_dem_wave_segments)

## Demographics, wave and items
mlog_dem_wave_items <- multinom(Orientation ~ Gender + Education + Age + Wave + 
                              ipcrtiv + imprich + ipeqopt + ipshabt + impsafe + impdiff +
                              ipfrule + ipudrst + ipmodst + ipgdtim + impfree + iphlppl +
                              ipsuces + ipstrgv + ipadvnt + ipbhprp + iprspot + iplylfr + 
                              impenv + imptrad + impfun, weights = reweight.nv, data = NLParty,
                              maxit = 200)

## ML Testing for differences between models
aov_ess <- anova(mlog_int, mlog_dem, mlog_dem_wave, mlog_dem_wave_segments, mlog_dem_wave_items, 
                 test = "Chisq")
